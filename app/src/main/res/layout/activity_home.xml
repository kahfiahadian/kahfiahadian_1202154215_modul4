<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="id.ac.telkomuniversity.student.kahfiahadian.kahfiahadian_1202154215_modul4.home">

    <Button
        android:id="@+id/listmahasiswa"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="148dp"
        android:text="List Mahasiswa"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="231dp"
        android:onClick="mahasiswa"/>

    <Button
        android:id="@+id/gambar"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/listmahasiswa"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="83dp"
        android:text="Cari Gambar"
        android:onClick="gambar"/>
</RelativeLayout>
